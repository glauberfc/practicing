{
  "name": "@abb/controller",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "introspect": "apollo-codegen introspect-schema http://localhost:4000 --output schema.json",
    "generate": "apollo-codegen generate src/**/*.tsx --schema schema.json --target typescript --output src/operation-result-types.ts",
    "gen-types": "yarn run introspect && yarn run generate"
  },
  "dependencies": {
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "react": "^16.6.1",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.6.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.3",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "apollo-codegen": "^0.20.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6"
  }
}
